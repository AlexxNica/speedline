{
  "name": "speedline",
  "version": "0.0.1",
  "description": "Get the speed index from chrome dev tool timeline files",
  "license": "MIT",
  "repository": "pmdartus/speedline",
  "author": {
    "name": "Pierre-Marie Dartus",
    "email": "dartus.pierremarie@gmail.com",
    "url": "github.com/pmdartus"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=5.0"
  },
