  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "fs-promise": "^0.5.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "ignores": [
